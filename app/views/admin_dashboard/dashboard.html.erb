<div id="layoutSidenav">
  <div id="layoutSidenav_content">
    <main>
      <% flash.each do |name, msg| %>
        <%= content_tag(:div, msg, class: "alert alert-success") if msg.length > 0 %>
      <% end %>
      <div class="container-fluid">
        <h1 class="mt-4">Dashboard</h1>
        <div class="row">
          <div class="col-sm-12">
            <section id="tabs">
              <div class="">
                <div class="row">
                  <div class="col-sm-9 ">
                    <nav>
                      <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                        <a class="nav-item nav-link dashboard-b1" id="nav-projects-tab" data-toggle="tab" href="#projects-table" role="tab" aria-controls="nav-home" aria-selected="true">
                          <div class="card-body ">Users</div>
                          <div class="card-footer d-flex align-items-center justify-content-between">
                            <div class="small text-white">View Details</div>
                            <div class="small text-white"><i class="fas fa-angle-right"></i></div>
                          </div>
                        </a>
                        <%# if current_user.role.eql?("Admin") && current_user.client_company.company_name.eql?("Ambquad") %>

                          <a  class="nav-item nav-link dashboard-admin-b2" id="nav-users-tab" data-toggle="tab" href="#users-table" role="tab" aria-controls="nav-profile" aria-selected="false">
                            <div class="card-body">Medicines</div>
                            <div class="card-footer d-flex align-items-center justify-content-between">
                              <div class="small text-white">View Details</div>
                              <div class="small text-white"><i class="fas fa-angle-right"></i></div>
                            </div>
                          </a>

                          <a class="nav-item nav-link dashboard-admin-b3" id="nav-companies-tab" data-toggle="tab" href="#companies-table" role="tab" aria-controls="nav-contact" aria-selected="false">
                            <div class="card-body">Categories</div>
                            <div class="card-footer d-flex align-items-center justify-content-between">
                              <div class="small text-white">View Details</div>
                              <div class="small text-white"><i class="fas fa-angle-right"></i></div>
                            </div>
                          </a>
                        <%# else %>
                          <a class="nav-item nav-link ml-2 dashboard-user-b2" id="client_company-tab" data-toggle="tab" href="#client_company" role="tab" aria-controls="nav-contact" aria-selected="false">
                            <div class="card-body">Sub Categories</div>
                            <div class="card-footer d-flex align-items-center justify-content-between">
                              <div class="small text-white">View Details</div>
                              <div class="small text-white"><i class="fas fa-angle-right"></i></div>
                            </div>
                          </a>
                          <div class="col-sm-4 "></div>
                        <%# end %>
                      </div>
                    </nav>
                    <div class="col-sm-3 "></div>
                  </div>
                </div>
